{
    "name": "toptracker",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "test": "vitest --run --passWithNoTests",
        "coverage": "vitest run --coverage",
        "prepare": "husky install",
        "lint": "eslint --fix --ext .ts,.vue . && prettier --write \"**/*.{.ts,vue}\""
    },
    "devDependencies": {
        "@nuxt/devtools": "^1.0.3",
        "@nuxtjs/eslint-module": "^4.1.0",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "@vitejs/plugin-vue": "^4.5.0",
        "@vitest/coverage-v8": "^0.34.6",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.54.0",
        "eslint-plugin-vue": "^9.18.1",
        "gsap": "^3.12.2",
        "husky": "^8.0.0",
        "lint-staged": "^15.1.0",
        "mongoose": "^8.0.3",
        "nuxt": "^3.8.2",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "tailwindcss": "^3.3.5",
        "vitest": "^0.34.6",
        "vue": "^3.3.8",
        "vue-router": "^4.2.5",
        "dotenv": "^16.3.1"
    },
    "lint-staged": {
        "**/*.{ts,vue}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "prettier": {
        "semi": true,
        "singleQuote": false,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "useTabs": true
    },
    "dependencies": {
        "mongoose": "^8.0.3"
    }
}
